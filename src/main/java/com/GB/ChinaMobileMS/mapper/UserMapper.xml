<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.GB.ChinaMobileMS.dao.UserMapper">

	<select id="findByUserName" parameterType="string" resultType="com.GB.ChinaMobileMS.entity.User">
		select *
		from user
		where user_name=#{userName}
	</select>
	

	<insert id="insertUser" parameterType="User"  >
		INSERT INTO user(user_name, account_name)
		VALUES (#{userName},#{accountName})
	</insert>
	
	<select id="listUser"  resultType="User">
		select * from user
		
	</select>
	
</mapper>